<a href="/admin/<%= controller.controller_name %>/<%= action_data.id %>/edit"><i class="fas fa-edit"></i></a>
<% if(action_data.status === 0)%>
  <a style="cursor:pointer;" onclick="onPublish(<%= action_data.id %>, '<%= controller.controller_name %>')"><i class="fa fa-bullhorn"></i></a>
<%end%>
<% if(action_data.status === 1)%>
  <a style="cursor:pointer;" onclick="onUnpublish(<%= action_data.id%>, '<%= controller.controller_name %>')"><i class="fa fa-undo"></i></a>
<%end%>
<a data-confirm="Are you sure you want to delete this item?" rel="nofollow" data-method="delete" href="/admin/<%= controller.controller_name %>/<%= action_data.id %>"><i class="fas fa-trash-alt"></i></a>